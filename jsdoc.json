{
  "source": {
    "include": ["./Ell-pollo-loco/models"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./Ell-pollo-loco/docs"
  }
} 